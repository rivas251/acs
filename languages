FREQUENCIES VARIABLES=SEX
  /ORDER=ANALYSIS.

COMPUTE AGEP_R = 0.
IF AGEP < 18 AGEP_R = 2.
IF AGEP >= 18 AGEP_R = 1.
EXECUTE.

VARIABLE LABELS
AGEP_R 'Age Group'.
VALUE LABELS
AGEP_R
1 '18+'
2 'Less than 18 years old'.

COMPUTE AGEP_R5 = 0.
IF AGEP < 5 AGEP_R5 = 1.
IF AGEP >= 5 AGEP_R5 = 2.
EXECUTE.

VARIABLE LABELS
AGEP_R5 'Age Group (5 years)'.
VALUE LABELS
AGEP_R5
1 'Less than 5 years old'
2 '5+'.

COMPUTE SD_COUNTY = 2.
IF (PUMA >= 7300 & PUMA <= 7399) SD_COUNTY = 1.
EXECUTE.

VARIABLE LABELS
SD_COUNTY 'San Diego County Flag'.
VALUE LABELS
SD_COUNTY
1 'San Diego County'
2 'Outside of San Diego County (CA)'.

COMPUTE SD_CITY = 3.
IF SD_COUNTY = 1 SD_CITY = 2.
IF (PUMA = 7310 | PUMA = 7311 | PUMA = 7311 | PUMA = 7312 | PUMA = 7315 | PUMA = 7316 | PUMA = 7317 | PUMA = 7318 | PUMA = 7322) SD_CITY = 1.
EXECUTE.

VARIABLE LABELS
SD_CITY 'San Diego City Flag'.
VALUE LABELS
SD_CITY
1 'San Diego City'
2 'Outside of San Diego City, within San Diego County'
3 'Outside of San Diego County (CA)'.

COMPUTE SD_CITY_CONTINOUS = 4.
IF SD_COUNTY = 1 SD_CITY_CONTINOUS = 3.
IF PUMA = 7322 SD_CITY_CONTINOUS = 2.
IF (PUMA = 7310 | PUMA = 7311 | PUMA = 7311 | PUMA = 7312 | PUMA = 7315 | PUMA = 7316 | PUMA = 7317 | PUMA = 7318) SD_CITY_CONTINOUS = 1.
EXECUTE.

VARIABLE LABELS
SD_CITY_CONTINOUS 'San Diego City Continous Flag'.
VALUE LABELS
SD_CITY_CONTINOUS
1 'San Diego City Continous'
2 'San Ysidro & Otay Mesa'
3 'Outside of San Diego City, within San Diego County'
4 'Outside of San Diego County (CA)'.

