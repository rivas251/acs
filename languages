COMPUTE ENG_PROF = 0.
IF ENG = 3 | ENG = 4 ENG_PROF = 1.
IF ENG = 1 | ENG = 2 | LANX = 2 ENG_PROF = 2.
IF AGEP < 5 ENG_PROF  = 3.
EXECUTE.

VARIABLE LABELS
ENG_PROF 'English Proficiency'.
VALUE LABELS
ENG_PROF
1 'Not well/Not at all English'
2 'Only/Very well/Well English'
3 'Less than 5 years old'.

COMPUTE LANG_DET_PROF = 0.
IF ENG_PROF = 1 LANG_DET_PROF = LANP.
IF ENG_PROF = 2 LANG_DET_PROF = 1.
EXECUTE.

